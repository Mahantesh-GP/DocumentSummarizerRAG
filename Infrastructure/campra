var prefixes = new HashSet<string>();
        await foreach (BlobItem blobItem in _containerClient.GetBlobsAsync(prefix: null, traits: BlobTraits.None, states: BlobStates.None))
        {
            var parts = blobItem.Name.Split('/');
            if (parts.Length > 1)
                prefixes.Add(parts[0]); // Folder name
        }

        return prefixes.OrderBy(x => x).ToList();
