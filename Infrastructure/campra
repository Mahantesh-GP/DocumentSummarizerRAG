var blobServiceClient = new BlobServiceClient("<your-connection-string>");
var containerClient = blobServiceClient.GetBlobContainerClient("<container-name>");
var blobClient = containerClient.GetBlobClient("eclosing/2021-25/filename.pdf");

var sasUri = blobClient.GenerateSasUri(BlobSasPermissions.Read, DateTimeOffset.UtcNow.AddHours(1));
Console.WriteLine(sasUri.ToString());
